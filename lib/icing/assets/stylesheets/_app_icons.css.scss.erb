@font-face {
	font-family: 'appicons';
	src:url(<%= asset_path("appicons.eot") %>);
	src:url(<%= asset_path("appicons.eot") + "?#iefix" %>) format('embedded-opentype'),
		url(<%= asset_path("appicons.woff") %>) format('woff'),
		url(<%= asset_path("appicons.ttf") %>) format('truetype'),
		url(<%= asset_path("appicons.svg") + "#appicons" %>) format('svg');
	font-weight: normal;
	font-style: normal;
}

.app-icon-sitefeed:before, .app-icon-signout:before, .app-icon-settings:before, .app-icon-search:before, .app-icon-tentstatus:before {
	font-family: 'appicons';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}
.app-icon-sitefeed:before {
	content: "\46";
}
.app-icon-signout:before {
	content: "\4c";
}
.app-icon-settings:before {
	content: "\50";
}
.app-icon-search:before {
	content: "\53";
}
.app-icon-tentstatus:before {
	content: "\54";
}
